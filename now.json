{
  "name": "hitlify",
  "version": 2,
  "alias": ["hitlify"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "public/index.html"
    }
  ]
}
